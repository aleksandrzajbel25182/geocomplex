<Window x:Class="geocomplex.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:geocomplex"
        xmlns:us="clr-namespace:geocomplex.UserContols"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1366
        ">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0*"/>
            <RowDefinition Height="737"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="1214"/>
        </Grid.ColumnDefinitions>
        <us:HamburgerMenu Grid.Column="0" Grid.Row="1"/>


        <ContentPresenter x:Name="contentPresenter"
                          Grid.Column="1"
                          Grid.Row="1"
                          Content="{Binding UserInterface}"/>

    </Grid>

</Window>
